{
  "title": "Amazon crawler",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "country": {
      "title": "Amazon market",
      "type": "string",
      "description": "Select your Amazon domain",
      "editor": "select",
      "default": "US",
      "enum": [
        "US",
        "UK",
        "DE",
        "ES",
        "FR",
        "IT",
        "IN",
        "CA"
      ],
      "enumTitles": ["amazon.com",
        "amazon.co.uk",
        "amazon.de",
        "amazon.es",
        "amazon.fr",
        "amazon.it",
        "amazon.in",
        "amazon.ca"]
    },

    "keywords": {
      "title": "Keywords",
      "type": "array",
      "description": "Keywords you would like to extract from Amazon",
      "prefill": ["Iphone X", "Samsung monitor 27 QHD"],
      "editor": "json"
    }
  },
  "required": [
    "country",
    "keywords"
  ]
}